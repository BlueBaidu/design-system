{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "test", "test:ts", "test:e2e"]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "{projectRoot}/packages/strapi-icons/dist",
        "{projectRoot}/packages/strapi-design-system/dist",
        "{projectRoot}/docs/storybook-static"
      ]
    }
  }
}
